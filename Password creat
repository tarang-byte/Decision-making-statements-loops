#include <iostream>
using namespace std;

int main() {
    string password = "@rajeev";
    string user_password;

    cout << "Enter the password: ";
    cin >> user_password;

    while (true) {
        if (user_password == password) 
        {
            cout << "System is Unlocked" << endl;
            break;
        } 
        else 
        {
            cout << "Incorrect Password.\n";
            cout << "Enter the password: ";
            cin >> user_password;
        }
    }

    return 0;
}
Output:
Enter the password: rachit
Incorrect Password.
Enter the password: @rajeev
System is Unlocked
